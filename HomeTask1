№ 3 
package com.company;

import java.util.Scanner;

public class Main {

public static void main(String[] args) {
Scanner scanner = new Scanner(System.in);
System.out.println("Ведите N ");
double N= scanner.nextInt();

if(N%2==0)
{
System.out.println("чётное");
}
else
{
System.out.println("нечётное");
}

}

}
