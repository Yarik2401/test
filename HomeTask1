№8 package com.company;
import javax.swing.*;
import java.util.Scanner;

public class Main {

public static void main(String[] args) {
Scanner scanner = new Scanner(System.in);
System.out.println("Ведите N ");
long N= scanner.nextLong();
System.out.println("Год рождения ="+" "+Year(N));
System.out.println("Вес ="+" "+Ves(N));
System.out.println("рост ="+" "+rost(N));
System.out.println("возвраст ="+" "+Age(N));
//System.out.println(Year(N));

// write your code here
}
public static long Year (long N){
N= ((N-N%10000000)/10000000);
return N;

// write your code here
}
public static long Ves (long N){
return (N%10000000)/100000;

// write your code here
}
public static long rost (long N){
return (N%100000)/100;

// write your code here
}
public static long Age (long N){
return (N%100);

// write your code here
}
}
